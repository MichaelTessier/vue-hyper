{
  "name": "vue-hyper-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "install:deps": "yarn install --frozen-lockfile",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.vue . && yarn stylelint 'src/**/*.{vue,css}'",
    "lint:fix": "yarn lint --fix",
    "codegen": "graphql-codegen",
    "codegen:supabase": "node ./scripts/codegen-supabase.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "@urql/exchange-auth": "^2.1.6",
    "@urql/vue": "^1.1.2",
    "graphql": "^16.8.1",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.5",
    "vue-hyper-components": "0.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-vue-urql": "^3.0.0",
    "@pinia/testing": "^0.1.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "happy-dom": "^12.10.3",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "shelljs": "^0.8.5",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.2.2",
    "unocss": "^0.57.3",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.22"
  }
}
